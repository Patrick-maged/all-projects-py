class HrEmployee(object) :
    def __init__(self,Age,Attrition,BusinessTravel,DailyRate,Department,DistanceFromHome,Education,EducationField,EmployeeCount,EmployeeNumber,EnvironmentSatisfaction,Gender,HourlyRate,JobInvolvement,JobLevel,JobRole,JobSatisfaction,MaritalStatus,MonthlyIncome,MonthlyRate,NumCompaniesWorked,Over18,OverTime,PercentSalaryHike,PerformanceRating,RelationshipSatisfaction,StandardHours,StockOptionLevel,TotalWorkingYears,TrainingTimesLastYear,WorkLifeBalance,YearsAtCompany,YearsInCurrentRole,YearsSinceLastPromotion,YearsWithCurrManager):
        self.Age=Age
        self.Attrition=Attrition
        self.BusinessTravel=BusinessTravel
        self.DailyRate=DailyRate
        self.Department=Department
        self.DistanceFromHome=DistanceFromHome
        self.Education=Education
        self.EducationField=EducationField
        self.EmployeeCount=EmployeeCount
        self.EmployeeNumber=EmployeeNumber
        self.EnvironmentSatisfaction=EnvironmentSatisfaction
        self.Gender=Gender
        self.HourlyRate=HourlyRate
        self.JobInvolvement=JobInvolvement
        self.JobLevel=JobLevel
        self.JobRole=JobRole
        self.JobSatisfaction=JobSatisfaction
        self.MaritalStatus=MaritalStatus
        self.MonthlyIncome=MonthlyIncome
        self.MonthlyRate=MonthlyRate
        self.NumCompaniesWorked=NumCompaniesWorked
        self.Over18=Over18
        self.OverTime=OverTime
        self.PercentSalaryHike=PercentSalaryHike
        self.PerformanceRating=PerformanceRating
        self.RelationshipSatisfaction=RelationshipSatisfaction
        self.StandardHours=StandardHours
        self.StockOptionLevel=StockOptionLevel
        self.TotalWorkingYears=TotalWorkingYears
        self.TrainingTimesLastYear=TrainingTimesLastYear
        self.WorkLifeBalance=WorkLifeBalance
        self.YearsAtCompany=YearsAtCompany
        self.YearsInCurrentRole=YearsInCurrentRole
        self.YearsSinceLastPromotion=YearsSinceLastPromotion
        self.YearsWithCurrManager=YearsWithCurrManager
with open('C:\\Users\\pggergis\\Downloads\\archive\\WA_Fn-UseC_-HR-Employee-Attrition.csv') as k:
    lines=k.readlines()
    Hr=[]
    for line in lines :
        cell=line.split(',')
        Age=cell[0]
        Attrition=cell[1]
        BusinessTravel=cell[2]
        DailyRate=cell[3]
        Department=cell[4]
        DistanceFromHome=cell[5]
        Education=cell[6]
        EducationField=cell[7]
        EmployeeCount=cell[8]
        EmployeeNumber=cell[9]
        EnvironmentSatisfaction=cell[10]
        Gender=cell[11]
        HourlyRate=cell[12]
        JobInvolvement=cell[13]
        JobLevel=cell[14]
        JobRole=cell[15]
        JobSatisfaction=cell[16]
        MaritalStatus=cell[17]
        MonthlyIncome=cell[18]
        MonthlyRate=cell[19]
        NumCompaniesWorked=cell[20]
        Over18=cell[21]
        OverTime=cell[22]
        PercentSalaryHike=cell[23]
        PerformanceRating=cell[24]
        RelationshipSatisfaction=cell[25]
        StandardHours=cell[26]
        StockOptionLevel=cell[27]
        TotalWorkingYears=cell[28]
        TrainingTimesLastYear=cell[29]
        WorkLifeBalance=cell[30]
        YearsAtCompany=cell[31]
        YearsInCurrentRole=cell[32]
        YearsSinceLastPromotion=cell[33]
        YearsWithCurrManager=cell[34]
        hr=HrEmployee(Age,Attrition,BusinessTravel,DailyRate,Department,DistanceFromHome,Education,EducationField,EmployeeCount,EmployeeNumber,EnvironmentSatisfaction,Gender,HourlyRate,JobInvolvement,JobLevel,JobRole,JobSatisfaction,MaritalStatus,MonthlyIncome,MonthlyRate,NumCompaniesWorked,Over18,OverTime,PercentSalaryHike,PerformanceRating,RelationshipSatisfaction,StandardHours,StockOptionLevel,TotalWorkingYears,TrainingTimesLastYear,WorkLifeBalance,YearsAtCompany,YearsInCurrentRole,YearsSinceLastPromotion,YearsWithCurrManager)
